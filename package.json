{
  "name": "io-pagopa-pm-mock",
  "version": "0.0.1",
  "description": "TODO",
  "repository": "https://github.com/pagopa/io-pagopa-pm-mock",
  "author": "https://pagopa.gov.it",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all -s clean generate lint build:*",
    "build:tsc": "tsc",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx --cache",
    "clean": "npm-run-all -s clean:*",
    "clean:dist": "rimraf dist",
    "clean:yaml-files": "rimraf generated/yaml-file/*.yaml",
    "clean:ts-from-yaml": "rimraf generated/FespCdService && rimraf generated/PagamentiTelematiciPspNodoservice && rimraf generated/nodeNm3io && rimraf generated/nodeNm3psp ",
    "generate": "npm-run-all -s generate:ts-from-yaml:*",
    "generate:ts-from-yaml:api": "shx mkdir -p generated/api && rimraf generated/api/* && gen-api-models --strict 0 --api-spec ./api.yaml --out-dir generated/api"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.2.7",
    "rimraf": "^2.6.2",
    "ts-jest": "^25.3.1",
    "@types/node": "10.14.1",
    "danger": "^8.0.0",
    "danger-plugin-digitalcitizenship": "*",
    "@pagopa/eslint-config": "^1.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^1.12.1",
    "tslint": "^5.1.0",
    "typescript": "^4.3.5",
    "npm-run-all": "^4.1.3",
    "shx": "^0.3.3"
  },
  "dependencies": {
    "io-ts": "^2.2.16",
    "fp-ts": "^2.11.1",
    "@pagopa/openapi-codegen-ts": "^10.0.6",
    "@pagopa/ts-commons": "^10.2.0"
  }
}
